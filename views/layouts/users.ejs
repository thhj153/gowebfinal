</div>
</header>
<!-- Main Content -->
<form action="users/register" method="POST" enctype="multipart/form-data">

    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <% for (var i=0; i < users.length; i++) { %>
                    <div class="post-preview">
                        <p class="post-meta">
                            <a href="/admin/<%= users[i]._id %>">
                                <%= users[i].username %>
                            </a>
                            <select name="grade" id="grade">
                                <option value="<%= users[i].grade %>">
                                    <%= users[i].grade %>
                                </option>
                                <option value="<%= grade.GINDGI %>">
                                    <%= grade.GINDGI %>
                                </option>
                                <option value="<%= grade.DAKDULGI %>">
                                    <%= grade.DAKDULGI %>
                                </option>
                                <option value="<%= grade.AMEBA %>">
                                    <%= grade.AMEBA %>
                                </option>
                            </select>
                        </p>
                    </div>
                    <hr />
                    <% } %>
                        <!-- Pager -->
                        <div class="clearfix">
                            <button type='submit' class="btn btn-primary float-right" href="#">SAVE &rarr;</button>