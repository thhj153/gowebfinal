<!DOCTYPE html>
<html lang="en">
<%- include('layouts/header'); -%>

    <body>
        <%- include('layouts/navbar'); -%>
            <!-- Page Header -->
            <header class="masthead" style="background-image: url('img/home-bg.jpg')">
                <div class="overlay"></div>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-8 col-md-10 mx-auto">
                            <div class="site-heading">
                                <h1>Clean Blog</h1>
                                <span class="subheading">A Blog Theme by Start Bootstrap</span>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <% include('layouts/listbar')
            <!-- Main Content -->
                <form action="users/register" method="POST" enctype="multipart/form-data">

                    <div class="container">
                        <div class="row">
                            <div class="col-lg-8 col-md-10 mx-auto">
                                <% for (var i=0; i < users.length; i++) { %>
                                    <div class="post-preview">
                                        <p class="post-meta">
                                            <a href="/admin/<%= users[i]._id %>">
                                                <%= users[i].username %>
                                            </a>
                                            <select name="grade" id="grade">
                                                <option value="<%= users[i].grade %>">
                                                    <%= users[i].grade %>
                                                </option>
                                                <option value="<%= grade.GINDGI %>">
                                                    <%= grade.GINDGI %>
                                                </option>
                                                <option value="<%= grade.DAKDULGI %>">
                                                    <%= grade.DAKDULGI %>
                                                </option>
                                                <option value="<%= grade.AMEBA %>">
                                                    <%= grade.AMEBA %>
                                                </option>
                                            </select>
                                        </p>
                                    </div>
                                    <hr />
                                    <% } %>


                                        <div class="clearfix">
                                            <a class="btn btn-primary float-right" href="#">Next List &rarr;</a>
                                        </div>
                            </div>
                        </div>
                </form>

                </div>

                <hr />

                <%- include('layouts/footer'); -%>
                    <%- include('layouts/scripts'); -%>
    </body>

</html>